package clase1;

public class Date {
	public Date(int day, int month, int year);
	/*Date that stores day, month and year.*/
	public int getDay();
	/*Method that attached to a Date object gets the day of that given Date.*/
	public int getMonth();
	/*Method that attached to a Date object gets the month of that given Date.*/
	public int getYear();
	public void setDay(int day);
	public void setMonth(int month);
	public void setYear(int year);
}