package paquete;

public class Vehiculo {
	public static void arrancar() {
	System.out.println("Brr... ¡Acaba de arrancar!");	
 }
	public static void main (String [] args) {
	arrancar();
 }
}//class

	public Vehiculo() {
		System.out.println("Brr... ¡Acaba de arrancar!");
	}
	public static void main (String [] args) {
		Vehiculo miMoto = new Vehiculo();
	}

}