Proceso sin_titulo
	DEFINIR ALTURA, BASE, AREA, PERIMETRO como REAL;
	Escribir "Mencione la altura del rectángulo";
	leer ALTURA;
	Escribir "Mencione la base del rectángulo";
	leer BASE;
	AREA <- ALTURA*BASE;
	PERIMETRO<- (ALTURA+BASE)*2;
	Escribir "Area: ", AREA;
	Escribir "Perímetro: ", PERIMETRO;
FinProceso
